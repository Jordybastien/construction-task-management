{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html"
    },
    {
      "source": "/storybook/(.*)",
      "destination": "/storybook/$1"
    },
    {
      "source": "/((?!storybook).*)",
      "destination": "/index.html"
    }
  ]
}